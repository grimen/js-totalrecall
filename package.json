{
    "name": "@grimen/totalrecall",
    "version": "0.1.1",
    "description": "A runtime step profiler - for Node/JavaScript.",
    "keywords": [
        "profiler",
        "step-profiler",
        "step-runtime-profiler",
        "runtime-profiler",
        "runtime",
        "step",
        "timer",
        "function",
        "mixin"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/grimen/js-totalrecall.git"
    },
    "bugs": {
        "url": "https://github.com/grimen/js-totalrecall/issues"
    },
    "homepage": "https://github.com/grimen/js-totalrecall#readme",
    "author": "Jonas Grimfelt <jonas@markable.ai> (https://github.com/grimen)",
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "test": "npx jest",
        "test-ci": "npx jest --runInBand --forceExit --detectOpenHandles --coverage",
        "test-dev": "npx jest --watch",
        "validate": "npx eslint \"**/*.js\"",
        "release": "./bin/release",
        "coverage": "npx jest --coverage",
        "coverage-codecov": "npx codecov"
    },
    "dependencies": {
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "codecov": "^3.6.1",
        "eslint": "^6.8.0",
        "jest": "^24.9.0"
    },
    "jest": {
        "bail": true,
        "verbose": true,
        "testRegex": "test_.*\\.js",
        "testEnvironment": "node"
    }
}
